<%- include('partials/header'); -%>
    <h1>
        <%= name %>
    </h1>
    <div class="parent">
        <div class="box">
            <div>
                <form action="/delete" method="post">
                <% newitem.forEach((item)=>{ %>
                    <div class="child">
                        <input type="checkbox" name="checkbox"  onChange="this.form.submit()" value=<%=item._id %>>
                        <p><%= item.name%></p>
                    </div>
                    <% }) %>
                </form>
            </div>
            <form action="/" method="post">
                <input type="text" name="items" class="input" placeholder="Add new todo" autocomplete="off">
                <button type="submit" name="button" class="Submit" value=<%= name%>></button>
            </form>
        </div>
    </div>
    <a href="/work">H</a>
<%- include('partials/footer'); -%>